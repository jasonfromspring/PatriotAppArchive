{
  "expo": {
    "name": "PatriotApp",
    "slug": "patriotappui",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.patriotapp.patriotapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.patriotapp.patriotapp",
      "permissions": [

      ],
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_fg.png",
        "backgroundImage": "./assets/icon_bg.png"
      }
    }
  }
}
